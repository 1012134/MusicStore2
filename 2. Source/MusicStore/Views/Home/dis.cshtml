@model IEnumerable<MusicStore.Models.Album>

@{
    ViewBag.Title = "dis";
}

<h2>dis</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Information)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LinkImage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReleaseDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BuyCounter)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDGenre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDPublisher)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Language)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
